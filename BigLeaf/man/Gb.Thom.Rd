## The function is currently defined as
function (ustar, k = 0.41) 
{
    Rb <- 6.2 * ustar^-0.667
    Gb <- 1/Rb
    kB <- Rb * (k * ustar)
    return(cbind(Rb, Gb, kB))
  }
}
